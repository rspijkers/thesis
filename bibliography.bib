@article{earlyRopes,
  title = {Colour rope model for extreme relativistic heavy ion collisions},
  journal = {Nuclear Physics B},
  volume = {245},
  pages = {449-468},
  year = {1984},
  issn = {0550-3213},
  doi = {https://doi.org/10.1016/0550-3213(84)90441-3},
  url = {https://www.sciencedirect.com/science/article/pii/0550321384904413},
  author = {T.S. Biro and H.B. Nielsen and J. Knoll},
  abstract = {We have constructed a cooperative model for the description of meson production in extreme relativistic heavy ion collisions. Two effects are essentially considered: (i) the simultaneous production of many colour sources due to the primary interactions leads to cooperative, i.e. the non-additive, formation of a multistring object, called a rope; (ii) the decay of this rope depends - through the Schwinger mechanism - on its initial colour charge. A scaling behavior with respect to the masses of the heavy ions is discussed. The lifetime of the ropes is found to be practically no longer for the A + A than for the p + p case.}
}